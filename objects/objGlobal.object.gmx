ball ++
else if ball &gt; 0 and global.Uppress
ball --

if ball != prevball
{
if ball = 0
with objChristina scrAssignSprites("Link")
else if ball = 1
with objChristina scrAssignSprites("Rink")

with objChristina
{
sprite_index = sprCharDown
dir = 270
slidetime = 0
shielding = false
swording = false
}
}

if global.Xpress
{
menu = 0
ball = 1
}
}
else
{
if ball &lt; 2 and global.Downpress
ball ++
else if ball &gt; 0 and global.Uppress
ball --
else if global.Zpress
{
if ball = 2
{
game_restart()
save_settings()
}
else
if ball = 1
{
menu = 1
ball = 0
}
else
{
pause = !pause
}
}
}
}
